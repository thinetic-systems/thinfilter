#!/usr/bin/make -f

DEB_PYTHON_SYSTEM=pysupport

DEB_UPDATE_RCD_PARAMS := "start 99 2 3 4 5 . stop 40 0 1 6 ."

include /usr/share/cdbs/1/rules/debhelper.mk
include /usr/share/cdbs/1/class/python-distutils.mk



binary-predeb/thinfilter::
	install -d debian/thinfilter/usr/sbin
	mv debian/thinfilter/usr/bin/thinfilter.py    debian/thinfilter/usr/sbin/thinfilter
	rmdir debian/thinfilter/usr/bin

	rm debian/thinfilter/usr/share/thinfilter/static/jquery.js
	dh_link usr/share/thinfilter/static/jquery-1.3.2.min.js usr/share/thinfilter/static/jquery.js

